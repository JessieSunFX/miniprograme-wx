<view>
  <view class="topContainer">
    <view class="basicInfo">
        <image src="../../img/userLogo.png" background-size="cover"></image>
        <text>{{phoneNumber}}</text>
    </view>
    <view class="propertyInfo">
        <view class="myPerformance" bindtap='performanceTapFun'>
            <image src="../../img/performanceBg.png" background-size="cover"></image>
            <view class="bigText"><text>{{orderQuantity}}</text>单</view>
            <view>
                我的业绩
                <text class="rt">{{month}}月</text>
            </view>
        </view>
        <view class="myDiamond" bindtap='diamondTapFun'>
            <image src="../../img/diamondBg.png" background-size="cover"></image>
            <view class="bigText ellipsis"><text>{{diamondNumber}}</text>钻</view>
            <view>
                我的钻石
                <text class="rt">可提现</text>
            </view>
        </view>
    </view>
  </view>
  <view class="weui-panel mainContainer">
      <!-- <view class="weui-panel__hd">小图文组合列表</view> -->
      <view class="weui-panel__bd">
          <view class="weui-media-box weui-media-box_small-appmsg">
              <view class="weui-cells weui-cells_in-small-appmsg">
                  <navigator url="../myClient/myClient" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                      <view class="weui-cell__hd">
                      <image src="../../img/clientIcon.png" style="width: 20px;height: 20px;margin-right: 10px;margin-top:5px;" /></view>
                      <view class="weui-cell__bd weui-cell_primary">
                          <view>我的客户</view>
                      </view>
                      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                  </navigator>
                  <navigator url="../personalInfor/personalInfor" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                      <view class="weui-cell__hd"><image src="../../img/inforIcon.png" style="width: 20px;height: 20px;margin-right: 10px;margin-top:5px;" /></view>
                      <view class="weui-cell__bd weui-cell_primary">
                          <view>个人信息</view>
                      </view>
                      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                  </navigator>
                  <navigator url="../bankCardInfor/bankCardInfor" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                      <view class="weui-cell__hd"><image src="../../img/cardIcon.png" style="width: 20px;height: 20px;margin-right: 10px;margin-top:5px;" /></view>
                      <view class="weui-cell__bd weui-cell_primary">
                          <view>银行卡</view>
                      </view>
                      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                  </navigator>
                  <navigator url="../faq/faq" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                      <view class="weui-cell__hd"><image src="../../img/questionIcon.png" style="width: 20px;height: 20px;margin-right: 10px;margin-top:5px;" /></view>
                      <view class="weui-cell__bd weui-cell_primary">
                          <view>常见问题</view>
                      </view>
                      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                  </navigator>
                  <navigator url="../aboutUs/aboutUs" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                      <view class="weui-cell__hd"><image src="../../img/aboutIcon.png" style="width: 20px;height: 20px;margin-right: 10px;margin-top:5px;" /></view>
                      <view class="weui-cell__bd weui-cell_primary">
                          <view>关于我们</view>
                      </view>
                      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
                  </navigator>
              </view>
          </view>
      </view>
  </view>

  <view class="buttonContainer">
      <button class="weui-btn" type="primary" bindtap='logoutBtn'>退出登录</button>
  </view>
</view>